<h1>Admin::Users#index</h1>
<p>Find me in app/views/admin/users/index.html.erb</p>

<table class="table table-borderless">
	<thead class="thead-dark">
		<tr>
			<th>会員ID</th>
			<th>会員名</th>
			<th>メールアドレス</th>
		</tr>
	</thead>

	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.id %></td>
				<td>
                	<%= link_to admin_user_path(user.id) do %>
					<%= user.name %>
					<% end %>
		    	</td>
		    	<td><%= user.email %></td>
            	</td>
			</tr>
		<% end %>
	</tbody>

</table>